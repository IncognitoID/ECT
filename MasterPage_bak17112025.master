            document.addEventListener("DOMContentLoaded", function () {
                loadlangsetting();
                // Get the current page path without the leading "/"
                var currentPagePath = window.location.pathname.substring(1);
                // Get the value of the "PageType" cookie
                var pageType = getCookie("Page_Type");
                // Check the value of the "PageType" cookie and modify the menu accordingly
                if (currentPagePath === "ComingSoon.aspx") {

                } else {
                    if (pageType === "Shopping") {
