                                if (parentListItem) {
                                    var grandparentListItem = parentListItem.parentElement.closest('li');
                                    var headerLink = grandparentListItem ? grandparentListItem.querySelector('a.header_border') : null;

                                    // If the header link exists, make it bold
                                    if (headerLink) {
                                        headerLink.classList.add('active-menu');
                                    } else {
                                        // If there is no header link, make the current link bold
                                        link.classList.add('active-menu');
                                    }
                                }
                            }
                        });

                    } else if (pageType === "MyOffice") {
                        // Get all menu links
                        var menuLinks = document.querySelectorAll('.main-menu-office a');

                        // Loop through each menu link and check if it matches the current page path
                        menuLinks.forEach(function (link) {
                            // Check if the link's href attribute matches the current page path
                            if (link.getAttribute('href').indexOf(currentPagePath) !== -1) {

                                //if (link.getAttribute('href') === currentPagePath) {
                                // Find the parent <li> element
                                var parentListItem = link.closest('li');

                                // If the parent <li> element exists, find its parent <li> and the header link
                                if (parentListItem) {
                                    var grandparentListItem = parentListItem.parentElement.closest('li');
                                    var headerLink = grandparentListItem ? grandparentListItem.querySelector('a.header_border') : null;

                                    // If the header link exists, make it bold
                                    if (headerLink) {
                                        headerLink.classList.add('active-menu');
                                    } else {
                                        // If there is no header link, make the current link bold
                                        link.classList.add('active-menu');
                                    }
                                }
                            }
                        });
                    }
                }
            });

            // Function to get cookie value by name
            function getCookie(cookieName) {
                var name = cookieName + "=";
                var decodedCookie = decodeURIComponent(document.cookie);
                var cookieArray = decodedCookie.split(';');

                for (var i = 0; i < cookieArray.length; i++) {
                    var cookie = cookieArray[i].trim();
                    if (cookie.indexOf(name) === 0) {
                        return cookie.substring(name.length, cookie.length);
                    }
                }
                return "";
            }

            function Ask_Confirmation_Change_Level() {
                var confirmbutton = document.getElementById("btn_confirm_change_bo_level");
                confirmbutton.click();
            }
        </script>

