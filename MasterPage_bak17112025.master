            document.getElementById("currentYear").textContent = new Date().getFullYear();

            document.addEventListener("DOMContentLoaded", function () {
                var id = getCookieValue("userid");
                if (id) {
                    document.getElementById("ctl25").classList.add('cart_no');
                } else {
                    document.getElementById("div_logout").classList.add('hidden');
                    document.getElementById("mobile_cart_2").classList.add('hidden');
                    document.getElementById("ctl25").classList.add('cart_no_transparent');
                }
